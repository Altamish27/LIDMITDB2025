        <shape android:shape="oval">
            <solid android:color="#9C27B0" />
            <stroke android:width="2dp" android:color="#FFFFFF" />
        </shape>
    </item>
    <item>
        <shape android:shape="oval">
            <solid android:color="#673AB7" />
            <stroke android:width="2dp" android:color="#FFFFFF" />
        </shape>
    </item>
</selector>
